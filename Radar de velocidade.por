programa {
  funcao inicio() {
    
    cadeia radar

    escreva ("em qual velocidade voce estar?: ")
    leia (radar)

    se (radar >= 81)
    {
      escreva ("voce estar acima da velocidade informada, voce foi multado! ")
    }
    senao
    {
      escreva ("voce esta no limite da velocidade")
    }
    
  }
}
